<body class="page-template page-template-template-landing page-template-template-landing-php page page-id-380 page-child parent-pageid-2 woocommerce-no-js site-dark header_type_logo_center header_space_no mobile_no right-click-disable hide-popup-download-link wpb-js-composer js-comp-ver-6.0.2 vc_responsive">
<!-- Google Tag Manager (noscript) -->
<noscript>
	<iframe src="https://www.googletagmanager.com/ns.html?id=GTM-593Q4HN"
			height="0" width="0" style="display:none;visibility:hidden"></iframe>
</noscript>
<!-- End Google Tag Manager (noscript) -->
	<div id="all" class="site">
		<div class="preloader">
			<div class="preloader_content">
				<div class="preloader_content__container">
					<p class="preloader_content__container__text">RAMOS FINE</p>
					<ul class="preloader_content__container__list count_4">
						<li class="preloader_content__container__list__item">photography</li>
						<li class="preloader_content__container__list__item">portraits</li>
						<li class="preloader_content__container__list__item">fashion</li>
						<li class="preloader_content__container__list__item">portraits</li>
					</ul>
				</div>
			</div>
		</div>
		<header class="site-header dark main-row">
			<div class="container-fluid">
				
				<div class="logo">
					<a href="/"><img width="250" height="55" src="/assets/images/signature-logo.svg" alt="Ramos Fine Portraits Signature Logo"></a>
				</div>
				<div class="fr">
					{%if lang=='es'%}
						{% set data=nav_es %}
					{% else %}
						{% set data=nav %}
					{% endif %}
					{% include "nav.njk" %}
					<div class="butter-button nav-button visible_menu">
						<div></div>
					</div>
				</div>
			</div>
		</header>
		<main class="main-row">
			<div class="container">
				{%- block content %}
					{{ content | safe }}
				{%- endblock %}
			</div>
		</main>
		{%if lang=='es'%}
			{% set data=footer_es %}
		{% else %}
			{% set data=footer %}
		{% endif %}
		{% include "footer.njk" %}
	</div>

	<div class="right-click-disable-message main-row">
		<div class="container">
			<div class="cell">
				<p style="text-align: center">
					<strong>
						<span style="font-size: 18px">Content is protected. Right-click function is disabled.</span>
					</strong>
				</p>
			</div>
		</div>
	</div>
	<script type='text/javascript' defer="defer" src='/assets/js/jquery.js?ver=1.12.4'></script>
	<script type="text/javascript" defer src="{% if site.productionEnvironment %}/assets/js/main.min.js{% else %}/assets/js/main.js{% endif %}"></script>
